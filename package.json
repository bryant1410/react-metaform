{
  "name": "react-metaform",
  "version": "0.2.6",
  "description": "A library for dynamically creating React.js forms out of metadata",
  "main": "./lib/index.js",
  "scripts": {
    "build-demo": "SET NODE_ENV=production&&babel-node ./tools/build-demo.js",
    "build-dist": "SET NODE_ENV=production&&babel-node ./tools/build-dist.js",
    "build-lib": "SET NODE_ENV=production&&babel-node ./tools/build-lib.js",
    "start-demo": "babel-node ./demo/Server.js",
    "start-demo-dev": "SET NODE_ENV=development&&babel-node ./demo/Server.js",
    "wpds": "webpack-dev-server --config ./webpack.config.demo.dev.js",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "test-chrome": "./node_modules/.bin/karma start --single-run --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gearz-lab/react-metaform.git"
  },
  "keywords": [
    "react",
    "form",
    "autoform",
    "metaform",
    "reactjs",
    "metadata"
  ],
  "author": "Andre Pena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gearz-lab/react-metaform/issues"
  },
  "homepage": "https://github.com/gearz-lab/react-metaform#readme",
  "dependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.5",
    "brace": "^0.5.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "formato": "^1.0.5",
    "js-beautify": "^1.5.10",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "react": "^0.13.3",
    "react-ace": "^2.3.0",
    "react-bootstrap": "^0.25.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^0.13.3",
    "react-select": "^0.6.7",
    "react-widgets": "^2.8.0",
    "underscore": "^1.8.3",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.2.0",
    "child-process-promise": "^1.1.0",
