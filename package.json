{
  "name": "react-metaform",
  "version": "0.9.4",
  "description": "A library for dynamically creating React.js forms out of metadata",
  "main": "./lib/index.js",
  "scripts": {
    "build-demo": "babel-node ./tools/build-demo.js",
    "build-dist": "babel-node ./tools/build-dist.js",
    "build-lib": "babel-node ./tools/build-lib.js",
    "start": "cross-env NODE_ENV=production babel-node ./demo/Server.js",
    "start-dev": "cross-env NODE_ENV=development babel-node ./demo/Server.js",
    "wpds": "webpack-dev-server --config ./webpack.config.demo.dev.js",
    "wpds-redux": "webpack-dev-server --config ./webpack.config.demo.redux.dev.js",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "test-chrome": "./node_modules/.bin/karma start --single-run --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gearz-lab/react-metaform.git"
  },
  "keywords": [
    "react",
    "form",
    "autoform",
    "metaform",
    "reactjs",
    "metadata"
  ],
  "author": "Andre Pena",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gearz-lab/react-metaform/issues"
  },
  "homepage": "https://github.com/gearz-lab/react-metaform#readme",
