    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "~0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.0.0",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "^1.6.0",
    "mocha": "^2.2.5",
    "npm": "^2.14.2",
    "phantomjs": "^1.9.17",
    "raw-loader": "^0.5.1",
    "rimraf-promise": "^1.0.0",
